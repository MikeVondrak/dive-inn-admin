{
  "name": "npm-workspaces-dive-inn",
  "version": "1.0.0",
  "author": "Mike Vondrak & Rick Haag",
  "license": "MIT",
  "engines": {
    "npm": "8.1.0"
  },
  "workspaces": [
    "./packages/admin-client",
    "./packages/public-client"
  ],
  "scripts": {
    "go": "concurrently \"npm run start -w ./packages/admin-client\" \"npm run start -w ./packages/public-client\"",
    "goadmin": "npm run start -w ./packages/admin-client",
    "wsup": "npm i && npm i -ws",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.6",
    "concurrently": "^6.3.0",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "apollo-client": "^2.6.10",
    "hasura-cli": "^2.0.9"
  }
}
