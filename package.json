{
  "name": "npm-workspaces-dive-inn",
  "version": "1.0.0",
  "author": "Mike Vondrak & Rick Haag",
  "license": "MIT",
  "engines": {
    "npm": "8.1.0"
  },
  "workspaces": [
    "./packages/admin-client",
    "./packages/public-client"
  ],
  "scripts": {
    "go": "concurrently \"npm run go -w ./packages/admin-client\" \"npm run go -w ./packages/public-client\"",
    "wsup": "npm i && npm i -ws",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "dependencies": {
    "concurrently": "^6.3.0"
  }
}
