{
  "name": "npm-workspaces-dive-inn",
  "version": "1.0.0",
  "author": "Mike Vondrak & Rick Haag",
  "license": "MIT",
  "engines": {
    "npm": "8.1.0"
  },
  "workspaces": [
    "./packages/admin-client",
    "./packages/public-client"
  ],
  "scripts": {
    "go": "concurrently \"npm run start -w ./packages/admin-client\" \"npm run start -w ./packages/public-client\"",
    "goadmin": "npm run start -w ./packages/admin-client",
    "gopublic": "npm run start -w ./packages/public-client",
    "storybook": "concurrently \"cd packages/admin-client && npm run storybook\" \"cd packages/public-client && npm run storybook\"",
    "wsup": "npm i && npm i -ws",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "gql_generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.6",
    "concurrently": "^6.3.0",
    "npm": "^8.1.4",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@graphql-codegen/cli": "^1.9.0",
    "@graphql-codegen/fragment-matcher": "3.2.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-apollo-angular": "3.3.1",
    "@graphql-codegen/typescript-operations": "2.2.1",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "apollo-client": "^2.6.10",
    "babel-loader": "^8.2.3",
    "hasura-cli": "^2.0.9"
  }
}
